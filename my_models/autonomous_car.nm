module autonomous_car

s: [1..6] init 1;

	[go] s=1 ->  1 : (s'=2);
	[stop] s=1 -> 1 : (s'=1);
	[go] s=2 ->  1 : (s'=3);
	[stop] s=2 -> 1 : (s'=2);
	[go] s=3 ->  1 : (s'=4);
	[stop] s=3 -> 1 : (s'=3);
	[go] s=4 ->  1 : (s'=5);
	[stop] s=4 -> 1 : (s'=4);
	[go] s=5 ->  1 : (s'=6);
	[stop] s=5 -> 1 : (s'=5);
	[go] s=6 ->  1 : (s'=6);
	[stop] s=6 -> 1 : (s'=6);

endmodule

label "a1" = s=1;
label "a2" = s=2;
label "a3" = s=3;
label "a4" = s=4;
label "a5" = s=5;
label "a6" = s=6;

